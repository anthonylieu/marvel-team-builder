<body>
    <section class="searchbar">
        <input type="text" />
        <button class="search"><img src="/images/ui_assets/search.png" alt="search" class="glass"/></button>
    </section>
    <main class="stat_wrapper">
        <div class="characters-container">
            {{#each characters as |character|}}
            <div class="character col col-sm-3 col-md-6">
                <h2>{{character.title}}</h2>
                <img src="/images/{{character.image}}" alt="{{character.title}}" class="char-image"/>
                <div class="chart col col-sm-3 col-md-6">{{character.intelligence}}</div>
                <div class="chart col col-sm-3 col-md-6">{{character.strength}}</div>
                <div class="chart col col-sm-3 col-md-6">{{character.speed}}</div>
                <div class="chart col col-sm-3 col-md-6">{{character.durability}}</div>
                <div class="chart col col-sm-3 col-md-6">{{character.energy}}</div>
                <div class="chart col col-sm-3 col-md-6">{{character.fighting_skills}}</div>
            </div>
            {{/each}}
        </div>
    </main>
</body>
